{
  "name": "turtle-cursor-effects",
  "type": "module",
  "description": "Old-school cursor effects for your browser built in modern JavaScript.",
  "version": "1.0.16",
  "main": "./dist/cjs.cjs",
  "module": "./dist/esm.js",
  "browser": "./dist/turtle-cursor-effects.umd.js",
  "types": "./types.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types.d.ts",
      "require": "./dist/cjs.cjs",
      "import": "./dist/esm.js",
      "browser": "./dist/turtle-cursor-effects.umd.js"
    }
  },
  "files": ["dist", "types.d.ts"],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TurtleGithubQuest/turtle-cursor-effects.git"
  },
  "author": "tholman",
  "contributors": [
    "turtle"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TurtleGithubQuest/turtle-cursor-effects/issues"
  },
  "homepage": "https://github.com/TurtleGithubQuest/turtle-cursor-effects#readme",
  "devDependencies": {
    "rollup": "^2.77.0",
    "rollup-plugin-terser": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "prepublish": "npm run build"
  }
}